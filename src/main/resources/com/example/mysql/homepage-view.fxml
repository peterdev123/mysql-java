<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="375.0" prefWidth="436.0" style="-fx-background-color: #27200d;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mysql.HelloController">
    <children>
        <VBox
                alignment="CENTER"
                prefHeight="200.0"
                prefWidth="100.0"
                spacing="30.0"
        AnchorPane.leftAnchor="10.0"
        AnchorPane.rightAnchor="10.0"
        AnchorPane.topAnchor="10.0"
        AnchorPane.bottomAnchor="10.0"
        >
        <children>
            <Label text="Welcome to Your Account Settings" textFill="#ff4500">
                <font>
                    <Font name="Georgia Bold" size="18.0" />
                </font>
            </Label>
            <Button
                    fx:id="btnAddProfile"
                    mnemonicParsing="false"
                    onAction="#onAddProfileClick"
                    style="-fx-background-color: #ff4500;"
                    text="ADD PROFILE"
                    textFill="#27200d"
            >
                <font>
                    <Font name="Georgia Bold" size="18.0" />
                </font>
                <cursor>
                    <Cursor fx:constant="HAND" />
                </cursor>
            </Button>
            <Button
                    fx:id="btnModifyAccount"
                    mnemonicParsing="false"
                    onAction="#onModifyClick"
                    style="-fx-background-color: #ff4500;"
                    text="MODIFY CURRENT ACCOUNT"
                    textFill="#27200d"
            >
                <font>
                    <Font name="Georgia Bold" size="18.0" />
                </font>
                <cursor>
                    <Cursor fx:constant="HAND" />
                </cursor>
            </Button>
        </children>
    </VBox>
</children>
        </AnchorPane>
